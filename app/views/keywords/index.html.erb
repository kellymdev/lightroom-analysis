<h2>Keywords</h2>

<svg class="keyword-chart" width=960 height=400></svg>

<table class="data-table">
  <tr>
    <th>Keyword</th>
    <th>Frequency</th>
  </tr>

  <% @keywords.each do |keyword| %>
    <tr>
      <td><%= keyword[:tag].name %></td>
      <td><%= keyword[:frequency] %></td>
    </tr>
  <% end %>
</table>
